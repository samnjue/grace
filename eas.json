{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "android": {
      "buildType": "app-bundle",
      "gradleCommand": ":app:bundleRelease",
      "env": {
        "MIN_SDK_VERSION": "21",
        "TARGET_SDK_VERSION": "38"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
